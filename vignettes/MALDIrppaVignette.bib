issn = {1176-9351},
journal = {Cancer informatics},
keywords = {isotope,maldi,mass resolution,mass spectrometry,peak capacity,peak quanti fi cation,seldi,simulation},
month = jan,
pages = {41--52},
pmid = {19305631},
title = {{Understanding the characteristics of mass spectrometry data through the use of simulation.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2657656\&tool=pmcentrez\&rendertype=abstract},
volume = {1},
